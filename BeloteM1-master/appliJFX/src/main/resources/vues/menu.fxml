<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="800.0"
          style="-fx-background-image: url('laTable/belote-background.jpg');" xmlns="http://javafx.com/javafx/10.0.1"
          fx:controller="vues.Menu">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Button mnemonicParsing="false" onAction="#seDeconnecter" text="Se Deconnecter" GridPane.columnIndex="2"
                GridPane.rowIndex="1"/>
        <Button mnemonicParsing="false" onAction="#voirLesConcours" text="Voir les concours" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="160.0"/>
            </GridPane.margin>
        </Button>
        <Text fx:id="titre" strokeWidth="0.0" text="SetTexte" textAlignment="CENTER" wrappingWidth="267.33339174574576"
              y="-100.0" GridPane.columnIndex="1">
            <font>
                <Font name="Georgia Bold" size="24.0"/>
            </font>
            <effect>
                <DropShadow/>
            </effect>
        </Text>
        <Button fx:id="histo" mnemonicParsing="false" onAction="#goToMonHistorique" text="Afficher mon historique"
                GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </children>
</GridPane>
